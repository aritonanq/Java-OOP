
package Pegawai;

public class Pegawai {
    private String nama, noKTP;
    
    Pegawai(String nama, String noKTP){
        this.nama = nama;
        this.noKTP = noKTP;
    }
    public String getNama(){
        return nama;
    }
    public String getNoKTP(){
        return noKTP;
    }
    public String toString(){
        return "\nNo. KTP\t\t: "+getNoKTP();
    }
    public double gaji(){
        return 0;
    }
}
